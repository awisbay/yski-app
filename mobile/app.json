{
  "expo": {
    "name": "YSKI App",
    "slug": "sahabat-khairat-indonesia",
    "scheme": "yski",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.yski.clicky"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.yski.clicky",
      "usesCleartextTraffic": true,
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset",
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "Izinkan akses galeri untuk memilih foto peralatan.",
          "cameraPermission": "Izinkan akses kamera untuk mengambil foto peralatan."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#10B981"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "apiUrl": "http://173.212.211.18:8080/api/v1",
      "googleMapsApiKey": "",
      "router": {},
      "eas": {
        "projectId": "333d16d5-6eac-4385-a216-5496296131a3"
      }
    },
    "owner": "wisbay"
  }
}
