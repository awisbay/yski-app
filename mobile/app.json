{
  "expo": {
    "name": "Yayasan Sahabat Khairat Indonesia",
    "slug": "clicky-foundation",
    "scheme": "clicky",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.yski.clicky"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#10B981"
      },
      "package": "org.yski.clicky"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset",
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "Izinkan akses galeri untuk memilih foto peralatan.",
          "cameraPermission": "Izinkan akses kamera untuk mengambil foto peralatan."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false
    },
    "extra": {
      "apiUrl": "http://173.212.211.18/api/v1",
      "googleMapsApiKey": ""
    }
  }
}
